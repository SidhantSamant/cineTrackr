{
    "expo": {
        "name": "Cine Trackr",
        "slug": "cineTrackr",
        "version": "1.0.0",
        "scheme": "cineTrackr",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "dark",
        "backgroundColor": "#121212",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#121212"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#121212"
            },
            "package": "com.sidhantsamant.cinetrackr"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            "expo-router",
            "expo-web-browser",
            "expo-font",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/icon.png",
                    "resizeMode": "contain",
                    "backgroundColor": "#121212"
                }
            ],
            [
                "expo-navigation-bar",
                {
                    "backgroundColor": "#121212",
                    "barStyle": "light"
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true,
            "tsconfigPaths": true
        }
    }
}
